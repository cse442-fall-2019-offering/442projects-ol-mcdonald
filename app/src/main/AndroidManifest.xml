<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.cse442.olmcdonald">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ItemDetailsActivity"></activity>
        <activity android:name=".SelectedCrop" />
        <activity android:name=".ItemAdapter" />
        <activity android:name=".Farmer" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ZipActivity" />
        <activity
            android:name=".LoginPageActivity"
            android:label="@string/title_activity_login_page"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>